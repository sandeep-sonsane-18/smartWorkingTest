{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      gender: \"\",\n      formSubmitted: false,\n      image: '',\n      property_types: [],\n      num_of_bed_selected: 1,\n      num_of_bath_selected: 1,\n      form: {\n        county: '',\n        country: '',\n        town: '',\n        postcode: '',\n        description: '',\n        displayable_name: '',\n        image: '',\n        num_of_bedrooms: '',\n        num_of_bathrooms: '',\n        price: '',\n        property_type: '',\n        for_rent_sale: ''\n      },\n      formErrors: {\n        county: '',\n        country: '',\n        town: '',\n        postcode: '',\n        description: '',\n        displayable_name: '',\n        image: '',\n        num_of_bedrooms: '',\n        num_of_bathrooms: '',\n        price: '',\n        property_type: '',\n        for_rent_sale: ''\n      },\n      errorsMessages: {\n        county: {\n          required: 'This field is required'\n        },\n        country: {\n          required: 'This field is required'\n        },\n        town: {\n          required: 'This field is required'\n        },\n        postcode: {\n          required: 'This field is required'\n        },\n        description: {\n          required: 'This field is required'\n        },\n        displayable_name: {\n          required: 'This field is required'\n        },\n        image: {\n          required: 'This field is required',\n          allowed: 'Only Jpg files allowed'\n        },\n        num_of_bedrooms: {\n          required: 'This field is required'\n        },\n        num_of_bathrooms: {\n          required: 'This field is required'\n        },\n        price: {\n          required: 'This field is required',\n          number: 'Only numbers are allowed'\n        },\n        property_type: {\n          required: 'This field is required'\n        },\n        for_rent_sale: {\n          required: 'This field is required'\n        }\n      }\n    };\n  },\n  methods: {\n    submitForm: function () {\n      this.form.image = this.image;\n      if (!this.validateForm(this.form)) {\n        return false;\n      }\n      axios.post('http://localhost:8000/api/savePropertyDetails', this.form, {\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(response => function () {\n        console.log(response);\n      }).catch(response => function () {\n        console.log(response);\n      });\n    },\n    handleFileUpload(event) {\n      this.image = event.target.files[0];\n    },\n    removeErrorMessage(tag) {\n      this.formErrors[tag] = '';\n    },\n    validateForm(data) {\n      var errorCount = 0;\n      if (data.county == '') {\n        this.formErrors.county = this.errorsMessages.county.required;\n        errorCount + 1;\n      }\n      if (data.country == '') {\n        this.formErrors.country = this.errorsMessages.country.required;\n        errorCount + 1;\n      }\n      if (data.postcode == '') {\n        this.formErrors.postcode = this.errorsMessages.postcode.required;\n        errorCount + 1;\n      }\n      if (data.description == '') {\n        this.formErrors.description = this.errorsMessages.description.required;\n        errorCount + 1;\n      }\n      if (data.displayable_name == '') {\n        this.formErrors.displayable_name = this.errorsMessages.displayable_name.required;\n        errorCount + 1;\n      }\n      if (this.image.length == 0) {\n        this.formErrors.image = this.errorsMessages.image.required;\n        errorCount + 1;\n      }\n      if (this.image != '' && this.image['type'] != 'image/jpeg') {\n        this.formErrors.image = this.errorsMessages.image.allowed;\n        errorCount + 1;\n      }\n      if (data.price == '') {\n        this.formErrors.price = this.errorsMessages.price.required;\n        errorCount + 1;\n      }\n      if (errorCount > 0) {\n        return false;\n      }\n    }\n  },\n  created() {\n    axios.get(\"http://localhost:8000/api/fetchPropertyTypes\").then(response => {\n      this.property_types = response.data;\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n};","map":{"version":3,"mappings":"AAiGI,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,aAAa,EAAE,KAAK;MACpBC,KAAK,EAAC,EAAE;MACRC,cAAc,EAAE,EAAE;MAClBC,mBAAmB,EAAC,CAAC;MACrBC,oBAAoB,EAAC,CAAC;MACtBC,IAAI,EAAE;QACJC,MAAM,EAAC,EAAE;QACTC,OAAO,EAAC,EAAE;QACVC,IAAI,EAAC,EAAE;QACPC,QAAQ,EAAC,EAAE;QACXC,WAAW,EAAC,EAAE;QACdC,gBAAgB,EAAC,EAAE;QACnBV,KAAK,EAAC,EAAE;QACRW,eAAe,EAAC,EAAE;QAClBC,gBAAgB,EAAC,EAAE;QACnBC,KAAK,EAAC,EAAE;QACRC,aAAa,EAAC,EAAE;QAChBC,aAAa,EAAC;MAChB,CAAC;MACDC,UAAU,EAAE;QACVX,MAAM,EAAC,EAAE;QACTC,OAAO,EAAC,EAAE;QACVC,IAAI,EAAC,EAAE;QACPC,QAAQ,EAAC,EAAE;QACXC,WAAW,EAAC,EAAE;QACdC,gBAAgB,EAAC,EAAE;QACnBV,KAAK,EAAC,EAAE;QACRW,eAAe,EAAC,EAAE;QAClBC,gBAAgB,EAAC,EAAE;QACnBC,KAAK,EAAC,EAAE;QACRC,aAAa,EAAC,EAAE;QAChBC,aAAa,EAAC;MAChB,CAAC;MACDE,cAAc,EAAE;QACdZ,MAAM,EAAC;UACHa,QAAQ,EAAC;QACb,CAAC;QACDZ,OAAO,EAAC;UACJY,QAAQ,EAAC;QACb,CAAC;QACDX,IAAI,EAAC;UACDW,QAAQ,EAAC;QACb,CAAC;QACDV,QAAQ,EAAC;UACLU,QAAQ,EAAC;QACb,CAAC;QACDT,WAAW,EAAC;UACRS,QAAQ,EAAC;QACb,CAAC;QACDR,gBAAgB,EAAC;UACbQ,QAAQ,EAAC;QACb,CAAC;QACDlB,KAAK,EAAC;UACFkB,QAAQ,EAAC,wBAAwB;UACjCC,OAAO,EAAC;QACZ,CAAC;QACDR,eAAe,EAAC;UACZO,QAAQ,EAAC;QACb,CAAC;QACDN,gBAAgB,EAAC;UACbM,QAAQ,EAAC;QACb,CAAC;QACDL,KAAK,EAAC;UACFK,QAAQ,EAAC,wBAAwB;UACjCE,MAAM,EAAC;QACX,CAAC;QACDN,aAAa,EAAC;UACVI,QAAQ,EAAC;QACb,CAAC;QACDH,aAAa,EAAC;UACVG,QAAQ,EAAC;QACb;MACF;IACF,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACPC,UAAU,EAAE,YAAY;MAEtB,IAAI,CAAClB,IAAI,CAACJ,KAAI,GAAI,IAAI,CAACA,KAAK;MAE5B,IAAG,CAAC,IAAI,CAACuB,YAAY,CAAC,IAAI,CAACnB,IAAI,CAAC,EAAC;QAC/B,OAAO,KAAK;MACd;MAEAV,KAAK,CAAC8B,IAAI,CAAC,+CAA+C,EAAC,IAAI,CAACpB,IAAI,EAClE;QACIqB,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACJ,EACC,CAACC,IAAI,CAACC,QAAO,IAAK,YAAU;QAC7BC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;