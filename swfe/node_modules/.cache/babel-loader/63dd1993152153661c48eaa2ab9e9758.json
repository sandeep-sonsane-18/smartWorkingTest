n        </select><br>\r\n        <span>Number of bathrooms: </span>\r\n        <select name=\"num_of_bathrooms\" id=\"num_of_bathrooms\" v-model=\"form.num_of_bathrooms\">\r\n            <option v-for=\"j in 6\" :key=\"j\" :value=\"j\">{{ j }}</option>\r\n        </select><br>\r\n        <span>Price: </span>\r\n        <input \r\n          v-model=\"price\"\r\n          type=\"text\"\r\n          placeholder=\"Enter your price\" \r\n        /><br>\r\n        <span>Property Type: </span>\r\n        <select name=\"property_type\" id=\"property_type\" v-model=\"form.property_type\">\r\n            <option v-for=\"v, index in property_types\" :key=\"index\" :value=\"v\">{{ v }}</option>\r\n        </select><br>\r\n        <span>For Sale / For Rent: </span>\r\n        <input \r\n          type=\"radio\" \r\n          v-model=\"form.for_rent_sale\" \r\n          value=\"rent\" \r\n        />\r\n        <label>For Rent</label>\r\n        <input \r\n          type=\"radio\" \r\n          v-model=\"form.for_rent_sale\" \r\n          value=\"sale\" \r\n        />\r\n        <label>For Sale</label><br>\r\n        <input \r\n          class=\"submit\" \r\n          type=\"submit\" \r\n          value=\"Submit\"\r\n        >\r\n      </form>\r\n      <div v-if=\"formSubmitted\">\r\n        <h3>Form Submitted</h3>\r\n        <p>Name: {{ name }}</p>\r\n        <p>Email: {{ email }}</p>\r\n        <p>Gender: {{ gender }}</p>\r\n        <small>Click on run to launch the app again.</small>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  <script>\r\n    import axios from 'axios'\r\n    export default {\r\n      data() {\r\n        return {\r\n          name: \"\",\r\n          email: \"\",\r\n          gender: \"\",\r\n          formSubmitted: false,\r\n          image:'',\r\n          property_types: ['Flat','Bungalow'],\r\n          form: new FormData({\r\n            county:'',\r\n            country:'',\r\n            town:'',\r\n            postcode:'',\r\n            description:'',\r\n            image:'',\r\n            num_of_bedrooms:'',\r\n            num_of_bathrooms:'',\r\n            price:'',\r\n            property_type:'',\r\n            for_rent_sale:''\r\n          }),\r\n        };\r\n      },\r\n      methods: {\r\n        submitForm: function () {\r\n          this.form.append('image',this.image);\r\n\r\n          axios.post('http://localhost:8000/api/savePropertyDetails',this.form,\r\n            {\r\n                headers: {\r\n                    'Content-Type': 'multipart/form-data'\r\n                }\r\n            }\r\n            ).then(function(){\r\n            console.log('SUCCESS!!');\r\n            })\r\n            .catch(function(){\r\n            console.log('FAILURE!!');\r\n            });\r\n        },\r\n        handleFileUpload( event ){\r\n            this.image = event.target.files[0];\r\n        }\r\n      },\r\n    };\r\n  </script>\r\n  <style>\r\n    form {\r\n      padding: 10px;\r\n      border: 2px solid black;\r\n      border-radius: 5px;\r\n    }\r\n  \r\n    input {\r\n      padding: 4px 8px;\r\n      margin: 4px;\r\n    }\r\n  \r\n    span {\r\n      font-size: 18px;\r\n      margin: 4px;\r\n      font-weight: 500;\r\n    }\r\n  \r\n    .submit {\r\n      font-size: 15px;\r\n      color: #fff;\r\n      background: #222;\r\n      padding: 6px 12px;\r\n      border: none;\r\n      margin-top: 8px;\r\n      cursor: pointer;\r\n      border-radius: 5px;\r\n    }\r\n  \r\n  </style>"]},"metadata":{},"sourceType":"module"}