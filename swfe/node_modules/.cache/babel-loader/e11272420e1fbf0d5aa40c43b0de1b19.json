\n          v-model=\"form.town\"\r\n          type=\"text\"\r\n          placeholder=\"Enter your town\"\r\n          @keyup=\"removeErrorMessage('town')\"\r\n        /><span>{{ formErrors.town }}</span><br>\r\n        <span>Postcode: </span>\r\n        <input \r\n          v-model=\"form.postcode\"\r\n          type=\"text\"\r\n          placeholder=\"Enter your postcode\"\r\n          @keyup=\"removeErrorMessage('postcode')\"\r\n        /><span>{{ formErrors.postcode }}</span><br>\r\n        <span>Description: </span>\r\n        <input \r\n          v-model=\"form.description\"\r\n          type=\"textarea\"\r\n          placeholder=\"Enter your description\"\r\n          @keyup=\"removeErrorMessage('description')\"\r\n        /><span>{{ formErrors.description }}</span><br>\r\n        <span>Image upload: </span>\r\n        <input \r\n        type=\"file\" id=\"file\" ref=\"file\" @change=\"handleFileUpload( $event )\" accept=\".jpg\" @keyup=\"removeErrorMessage('image')\"\r\n        /><span>{{ formErrors.image }}</span><br>\r\n        <span>Number of bedrooms: </span>\r\n        <select name=\"num_of_bedrooms\" id=\"num_of_bedrooms\" v-model=\"form.num_of_bedrooms\">\r\n            <option v-for=\"i in 6\" :key=\"i\" :value=\"i\">{{ i }}</option>\r\n        </select><span>{{ formErrors.county }}</span><br>\r\n        <span>Number of bathrooms: </span>\r\n        <select name=\"num_of_bathrooms\" id=\"num_of_bathrooms\" v-model=\"form.num_of_bathrooms\">\r\n            <option v-for=\"j in 6\" :key=\"j\" :value=\"j\">{{ j }}</option>\r\n        </select><span>{{ formErrors.county }}</span><br>\r\n        <span>Price: </span>\r\n        <input \r\n          v-model=\"price\"\r\n          type=\"number\"\r\n          placeholder=\"Enter your price\"\r\n          @keyup=\"removeErrorMessage('price')\" \r\n        /><span>{{ formErrors.county }}</span><br>\r\n        <span>Property Type: </span>\r\n        <select name=\"property_type\" id=\"property_type\" v-model=\"form.property_type\">\r\n            <option v-for=\"v, index in property_types\" :key=\"index\" :value=\"v\">{{ v }}</option>\r\n        </select><span>{{ formErrors.county }}</span><br>\r\n        <span>For Sale / For Rent: </span>\r\n        <input \r\n          type=\"radio\" \r\n          v-model=\"form.for_rent_sale\" \r\n          value=\"rent\" \r\n        />\r\n        <label>For Rent</label>\r\n        <input \r\n          type=\"radio\" \r\n          v-model=\"form.for_rent_sale\" \r\n          value=\"sale\" \r\n        />\r\n        <label>For Sale</label><span>{{ formErrors.county }}</span><br>\r\n        <input \r\n          class=\"submit\" \r\n          type=\"submit\" \r\n          value=\"Submit\"\r\n        >\r\n      </form>\r\n      <div v-if=\"formSubmitted\">\r\n        <h3>Form Submitted</h3>\r\n        <p>Name: {{ name }}</p>\r\n        <p>Email: {{ email }}</p>\r\n        <p>Gender: {{ gender }}</p>\r\n        <small>Click on run to launch the app again.</small>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  <script>\r\n    import axios from 'axios'\r\n    export default {\r\n      data() {\r\n        return {\r\n          name: \"\",\r\n          email: \"\",\r\n          gender: \"\",\r\n          formSubmitted: false,\r\n          image:'',\r\n          property_types: [],\r\n          form: {\r\n            county:'',\r\n            country:'',\r\n            town:'',\r\n            postcode:'',\r\n            description:'',\r\n            image:'',\r\n            num_of_bedrooms:'',\r\n            num_of_bathrooms:'',\r\n            price:'',\r\n            property_type:'',\r\n            for_rent_sale:''\r\n          },\r\n          formErrors: {\r\n            county:'',\r\n            country:'',\r\n            town:'',\r\n            postcode:'',\r\n            description:'',\r\n            image:'',\r\n            num_of_bedrooms:'',\r\n            num_of_bathrooms:'',\r\n            price:'',\r\n            property_type:'',\r\n            for_rent_sale:''\r\n          },\r\n          errorsMessages: {\r\n            county:{\r\n                required:'This field is required',\r\n            },\r\n            country:{\r\n                required:'This field is required',\r\n            },\r\n            town:{\r\n                required:'This field is required',\r\n            },\r\n            postcode:{\r\n                required:'This field is required',\r\n            },\r\n            description:{\r\n                required:'This field is required',\r\n            },\r\n            image:{\r\n                required:'This field is required',\r\n                allowed:'Only Jpg files allowed'\r\n            },\r\n            num_of_bedrooms:{\r\n                required:'This field is required',\r\n            },\r\n            num_of_bathrooms:{\r\n                required:'This field is required',\r\n            },\r\n            price:{\r\n                required:'This field is required',\r\n                number:'Only numbers are allowed'\r\n            },\r\n            property_type:{\r\n                required:'This field is required',\r\n            },\r\n            for_rent_sale:{\r\n                required:'This field is required',\r\n            }\r\n          }\r\n        };\r\n      },\r\n      methods: {\r\n        submitForm: function () {\r\n            \r\n          this.form.image = this.image;\r\n\r\n          axios.post('http://localhost:8000/api/savePropertyDetails',this.form,\r\n            {\r\n                headers: {\r\n                    'Content-Type': 'multipart/form-data'\r\n                }\r\n            }\r\n            ).then(response => function(){\r\n            console.log(response);\r\n            })\r\n            .catch(response => function(){\r\n            console.log(response);\r\n            });\r\n        },\r\n        handleFileUpload( event ){\r\n            this.image = event.target.files[0];\r\n        },\r\n    