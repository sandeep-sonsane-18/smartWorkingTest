ss=\"submit\" \r\n          type=\"submit\" \r\n          value=\"Submit\"\r\n        >\r\n      </form>\r\n      <div v-if=\"formSubmitted\">\r\n        <h3>Form Submitted</h3>\r\n        <p>Name: {{ name }}</p>\r\n        <p>Email: {{ email }}</p>\r\n        <p>Gender: {{ gender }}</p>\r\n        <small>Click on run to launch the app again.</small>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  <script>\r\n    export default {\r\n      data() {\r\n        return {\r\n          name: \"\",\r\n          email: \"\",\r\n          gender: \"\",\r\n          formSubmitted: false,\r\n          property_types: ['Flat','Bungalow'],\r\n          form: new FormData({\r\n            'county':'',\r\n            'country':'',\r\n            'town':'',\r\n            'postcode':'',\r\n            'description':'',\r\n            'image':'',\r\n            'num_of_bedrooms':'',\r\n            'num_of_bathrooms':'',\r\n            'price':'',\r\n            'property_type':'',\r\n            'for_rent_sale':''\r\n          }),\r\n        };\r\n      },\r\n      methods: {\r\n        submitForm: function () {\r\n          this.formSubmitted = true\r\n        }\r\n      },\r\n    };\r\n  </script>\r\n  <style>\r\n    form {\r\n      padding: 10px;\r\n      border: 2px solid black;\r\n      border-radius: 5px;\r\n    }\r\n  \r\n    input {\r\n      padding: 4px 8px;\r\n      margin: 4px;\r\n    }\r\n  \r\n    span {\r\n      font-size: 18px;\r\n      margin: 4px;\r\n      font-weight: 500;\r\n    }\r\n  \r\n    .submit {\r\n      font-size: 15px;\r\n      color: #fff;\r\n      background: #222;\r\n      padding: 6px 12px;\r\n      border: none;\r\n      margin-top: 8px;\r\n      cursor: pointer;\r\n      border-radius: 5px;\r\n    }\r\n  \r\n  </style>"]},"metadata":{},"sourceType":"module"}